{
    "name": "ResumeVideo",
    "description": "__MSG_description__",
    "version": "1.1",
    "manifest_version": 2,
    "permissions": [
        "storage",
        "activeTab",
        "contextMenus"
    ],
    "optional_permissions": [
        "<all_urls>"
    ],
    "default_locale":"en",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "commands": {
        "stock_video": {
            "description": "Stock playing video",
            "suggested_key": {
                "default": "Ctrl+Shift+S"
            }
        }
    },
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    }
}